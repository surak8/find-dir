{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "debug-current",
			"program": "${file}",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
			"args": [
				"--debug",
				"--verbose",
				"%USERPROFILE%/source",
				"/js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "debug-it",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/main.js",
			"args": [
				"/js/prod",
				"/js/github/rik"
			]
		}
	]
}
