	"dependencies": {
		"yargs": "^17.7.2"
	},
	"devDependencies": {},
	"scripts": {
		"clean": "yarn clean:0&&yarn clean:1&&yarn clean:2",
		"clean:0": "IF EXIST node_modules RD /Q/S node_modules",
		"clean:1": "IF EXIST yarn.lock DEL /Q yarn.lock",
		"clean:2": "IF EXIST out RD /Q/S out"
	}
}
